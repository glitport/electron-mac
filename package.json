{
  "name": "electron-mac",
  "version": "1.0.0",
  "description": "Electron Test Mac",
  "author": "glitport",
  "private": true,
  "homepage": "./",
  "main": "public/electron/main.js",
  "scripts": {
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder",
    "app:linux": "electron-builder build -l --x64 --arm64 --armv7l",
    "app:mac": "electron-builder build -m --publish=never",
    "app:win": "electron-builder build -w"
  },
  "devDependencies": {
    "electron": "^34.0.1",
    "electron-builder": "^25.1.8"
  }
}
